import socket
s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
ssl_sock = ssl.wrap_socket(s, cert_reqs=ssl.CERT_REQUIRED,ca_certs='example.pem')
import ssl
ssl_sock = ssl.wrap_socket(s, cert_reqs=ssl.CERT_REQUIRED,ca_certs='example.pem')
ssl_sock.connect((172.0.17.2, 9443))
ssl_sock.connect(('172.0.17.2', 9443))
ssl_sock = ssl.wrap_socket(s, cert_reqs=ssl.CERT_REQUIRED,ca_certs='clientCert.pem')
quit()
